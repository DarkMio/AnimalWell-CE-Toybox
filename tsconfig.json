{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "types": [],
    "strict": true
  },
  "tstl": {
    "noHeader": true,
    "luaTarget": "5.3",
    "noImplicitSelf": true,
    "luaLibImport": "inline",
  }
}